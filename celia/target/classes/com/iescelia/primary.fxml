<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0"
   xmlns="http://javafx.com/javafx/23.0.1"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iescelia.PrimaryController">
   <children>
      <Label text="GESTION DE DISPOSITIVOS" />
      <Button mnemonicParsing="false" onAction="#verTodos" text="Ver todos" />
      <TextArea fx:id="salida" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="primaryButton" onAction="#anadirDispositivo" text="Añadir Dispositivo" />
      <Button fx:id="secondaryButton" onAction="#anadirOrdenador" text="Añadir Ordenador" />
      <TextField fx:id="busco" />
      <Button mnemonicParsing="false" onAction="#buscar" text="Buscar por ID" />
      <TextField fx:id="eliminar" prefHeight="25.0" prefWidth="114.0" />
      <Button mnemonicParsing="false" onAction="#eliminado" text="Eliminar" />
      <TextField fx:id="txtid" />
      <Button mnemonicParsing="false" onAction="#botonEstado" text="Modificar Estado" />
      <TextField fx:id="modificarDis" layoutX="30.0" layoutY="630.0" />
      <Button layoutX="73.0" layoutY="676.0" mnemonicParsing="false" onAction="#botonModificar" text="Modificar Dispositivo" />
      <Button onAction="#eliminarTodo" text="Eliminar todos los datos" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
